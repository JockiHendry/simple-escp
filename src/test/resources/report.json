{
    "pageFormat": {
        "pageWidth": 50,
        "pageLength": 13,
        "usePageLengthFromPrinter": false
    },
    "template": {
        "header": [
            "   %{176 R3}       Company Name                 Page %{PAGE_NO}",
            " %{176 R7}      ",
            "%{176 R9}    Invoice No: ${invoiceNo:10}",
            "  %{176 R5}       ",
            "   %{176 R3}        "
        ],
        "detail": [
            {
                "table": "table_source",
                "border": true,
                "columns": [
                    { "source": "code", "width": 9  },
                    { "source": "name", "width": 34, "wrap": true },
                    { "source": "qty",  "width": 6  }
                ]
            },
            "                                                  ",
            "   (Signature)                  (Signature)       "
        ]
    }
}