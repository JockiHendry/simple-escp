{
    "pageFormat": {
        "pageWidth": 50,
        "pageLength": 13,
        "usePageLengthFromPrinter": false
    },
    "template": {
        "header": [
            "   %{176 R3}     A%{BOLD}Company Name%{BOLD}A               Page %{PAGE_NO}",
            " %{176 R7}  B%{ITALIC}TEST%{ITALIC}B B%{UNDERLINE}TEST%{UNDERLINE}B B%{BOLD}BOLD%{BOLD}B ",
            "%{176 R9}    Invoice No: ${invoiceNo:10}",
            "  %{176 R5}       ",
            "   %{176 R3}        "
        ],
        "detail": [
            {
                "table": "table_source",
                "border": true,
                "columns": [
                    { "source": "row :: right", "width": 4 },
                    { "source": "code", "width": 9  },
                    { "source": "name", "width": 30, "wrap": true },
                    { "source": "qty",  "width": 6  }
                ]
            }
        ],
        "lastPageFooter": [
            "",
            "    Signed By                   Received By       ",
            "                                                  ",
            "   (Signature)                  (Signature)       "
        ]
    }
}